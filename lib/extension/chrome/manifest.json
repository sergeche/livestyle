{
	"name": "Emmet LiveStyle",
	"version": "1.0.0",
	"description": "Bi-directional live CSS editing",
	"update_url": "http://download.emmet.io/livestyle/chrome/updates.xml",

	"devtools_page": "devtools.html",
	"background": {
		"scripts": ["background.js"]
	},

	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"permissions": [
		"storage",
		"tabs",
		"http://*/",
		"https://*/"
	],

	"manifest_version": 2,
	"minimum_chrome_version": "21"
}
