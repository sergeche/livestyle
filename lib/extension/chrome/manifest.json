{
	"name": "Emmet LiveStyle",
	"version": "0.8.0",
	"description": "Live bi-directional CSS editing of new generation",

	"devtools_page": "devtools.html",
	"background": {
		"page": "background.html"
	},

	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"permissions": [
		"storage",
		"tabs",
		"http://*/",
		"https://*/"
	],

	"manifest_version": 2,
	"minimum_chrome_version": "26"
}
